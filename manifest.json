{
  "manifest_version": 2,
  "name": "SOCKS5 Proxy Toggle",
  "version": "1.0",
  "permissions": [
    "proxy",
    "storage",
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon.png"
  }
}
